{
  "name": "poliakovmaksym.github.io",
  "version": "0.0.8",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:ui": "majestic",
    "test:watch": "react-scripts test",
    "test:analysis": "react-scripts test --watchAll=false --runInBand --verbose --logHeapUsage --slowTestThreshold=3 --detectOpenHandles",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "test:coverage": "npm run test -- --coverage --watchAll=false --runInBand",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix --max-warnings=0 --quiet",
    "prettier": "prettier --write . --config ./.prettierrc",
    "prettier:check": "prettier --check . --config ./.prettierrc",
    "copy-files": "copy-files-from-to",
    "analyze-bundle": "npm run build -- --environment INCLUDE_BUNDLE_ANALYZE"
  },
  "dependencies": {
    "@emotion/react": "11.14.0",
    "@emotion/styled": "11.14.0",
    "@mui/icons-material": "7.2.0",
    "@mui/material": "7.2.0",
    "classnames": "2.5.1",
    "motion": "12.6.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.6.0",
    "react-scripts": "5.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.25.0",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.17",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "@typescript-eslint/eslint-plugin": "8.38.0",
    "@typescript-eslint/parser": "8.38.0",
    "copy-files-from-to": "3.12.1",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "jest-fail-on-console": "3.3.1",
    "majestic": "1.8.1",
    "prettier": "3.5.3",
    "typescript": "4.9.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
